apiVersion: v1
kind: Secret
metadata:
  name: paperless-secret
  labels:
    app: paperless
type: Opaque
stringData:
  # IMPORTANT: Copy this file to secret.yaml and replace with actual values
  # DO NOT commit secret.yaml to version control
  PAPERLESS_DBPASS: "your-secure-database-password-here"
  PAPERLESS_SECRET_KEY: "your-secure-secret-key-here-min-50-chars"
